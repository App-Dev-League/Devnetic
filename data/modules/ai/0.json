                {
                    "type": "text",
                    "content": "[[h3]]Number Operators[[/]]"
                },
                {
                    "type": "text",
                    "content": "➔ x + y: The sum of two numbers"
                },
                {
                    "type": "text",
                    "content": "➔ x - y: The difference of two numbers"
                },
                {
                    "type": "text",
                    "content": "➔ x * y: The product of two numbers"
                },
                {
                    "type": "text",
                    "content": "➔ x / y: The quotient of two numbers"
                },
                {
                    "type": "text",
                    "content":"➔ x // y: The quotient of two numbers rounded down to an integer"
                },
                {
                    "type": "text",
                    "content": "➔ x % y: “modulo”, The remainder when x is divided by y"
                },
                {
                    "type": "text",
                    "content": "➔ x ** y: x raised to the y power"
                },
                {
                    "type": "text",
                    "content": "➔ -x: x in its negated form (-1 * x)"
                },
                {
                    "type": "text",
                    "content": "➔ abs(x): The absolute value of a number"
                }
            ]
        }
