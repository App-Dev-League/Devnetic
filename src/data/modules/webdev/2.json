            "elements": [
                {
                    "type": "text",
                    "content": "[[h3]]Purpose of HTML[[/]]"
                },
                {
                    "type": "text",
                    "content": "Stands for: [[b]]H[[/]]yper [[b]]T[[/]]ext [[b]]M[[/]]arkup [[b]]L[[/]]anguage"
                },
                {
                    "type": "text",
                    "content": "HTML designs the layout of text and structure of the web page."
                },
                {
                    "type": "text",
                    "content": "Formats content (images, videos, external links, etc.)"
                },
                {
                    "type": "image",
                    "src": "/data/modules/webdev-assets/img2/1-0.png",
                    "box-shadow": "0px 0px 20px 7px #686868"
